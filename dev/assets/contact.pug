extends _templates/layout.pug

block content
	.contact-section.snj-container-fluid
		//- .snj-row
		.row
			.center-wrapper
				section.about-block.offset-1.snj-col-10.offset-md-2.snj-col-md-5
					h2.subtitle-block.sub-1 Get in Touch
					h2.subtitle-block.sub-2 Contact Us
					h1.title-block Let’s create great things
					.arrow-container
						+arrow-right("/", "Drop us a line")

				aside.social-block.snj-col-4.offset-1.snj-col-lg-1.offset-lg-1
					h2.subtitle-block Social
					ul
						li Instagram
						li Facebook
						li Twitter
						li Linkedin

				aside.info-block-wrapper.snj-col-4.offset-2.snj-col-md-9.offset-md-1
					.info-block
						h3 Office
						p No. 11, Jumeirah Centre Jumeirah 1, Dubai, UAE P.O Box 5182
					.info-block
						h3 Number
						p +971 (0) 4 330 0311
					.info-block
						p © Copyright 2019
	a(href="ourwork.html")
		button.btn-wrapper.btn-pages
			span.btn-animate-up Our Work
			
