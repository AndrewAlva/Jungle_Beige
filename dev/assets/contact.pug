extends _templates/layout.pug


block content
	.contact-section.snj-container-fluid
		//- .snj-row
		.row
			.center-wrapper
				a.about-block.offset-1.snj-col-10.offset-md-2.snj-col-md-5(href="message.html")
					h2.subtitle-block.sub-1.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="0" data-cascade-delay="0") Get in touch

					h2.subtitle-block.sub-2.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="0" data-cascade-delay="0") Contact Us

					h1.title-block.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="0" data-cascade-delay="0") Let’s link. Let’s build…

					.arrow-container.jb-cascade(data-cascade="fade-right" data-cascade-delay-mobile="1" data-cascade-delay="1")
						.arrow-right
							.arrow-wrapper
								.rectangle-arrow
								+icon-arrow
							span Drop us a line

				aside.social-block.snj-col-4.offset-1.snj-col-md-2.offset-lg-1

					h2.subtitle-block.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="2" data-cascade-delay="1") Social

					span.line-item
						.line.jb-cascade(data-cascade="line-full-height" data-cascade-delay-mobile="3" data-cascade-delay="1")
					ul
						li.jb-cascade(data-cascade="fade-right")
							a(href="https://www.instagram.com/junglebeige/") Instagram
						li.jb-cascade(data-cascade="fade-right")
							a(href="#") Facebook
						li.jb-cascade(data-cascade="fade-right")
							a(href="#") Twitter
						li.jb-cascade(data-cascade="fade-right")
							a(href="https://www.linkedin.com/company/jungle-beige") Linkedin

				aside.info-block-wrapper.snj-col-4.offset-2.snj-col-md-9.offset-md-1
					.info-block.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="2" data-cascade-delay="2")
						h3 Office
						p Dubai Design District<br> Building 7, 502A<br>Dubai, U.A.E. 

					.info-block.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="3" data-cascade-delay="3")
						h3 Number
						a(href="tel:+971043300311") +971 (0) 4 330 0311
					.info-block.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="4" data-cascade-delay="4")
						p © Copyright 2020

	a.navigation-btn.btn-pages(href="ourwork.html")
		span.animate-up.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="1" data-cascade-delay="1") Our Work



block footer
	+footer('')