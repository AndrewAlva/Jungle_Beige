extends _templates/layout.pug

block content
	.index-wrapper
		.background-wrapper.home-bg

		//- mobile display
		.display-wrapper
			.display-type.jb-cascade(data-cascade="scale-small" data-cascade-delay-mobile="0" data-cascade-delay="0")
				h2.display-w w
				h2.display-e e
			.title-wrapper
				.span-container.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="3" data-cascade-delay="3")
					span create
					span &
					span consult
					//- span.separator-line.jb-cascade(data-cascade="line-full" data-cascade-delay-mobile="4" data-cascade-delay="4")
					//- 	.line
			p.about-title.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="4" data-cascade-delay="4") We <br> create and consult for lifestyle cultures and industries such as fashion, hospitality, entertainment and more. 
	//- para caundo se quiere regresar. la pagina anterior
	//- a(href="javascript:history.back()")
	a.btn-wrapper.btn-pages(href="ourwork.html")
		span.animate-up.jb-cascade(data-cascade="fade-up" data-cascade-delay-mobile="1" data-cascade-delay="1") Our Work
block footer
	+footer('')
		